{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
//    "outDir": "../SonarApp/app/src/main/assets/out-tsc",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "lib": [
      "es6",
      "dom"
    ],
    "paths": { "@angular/*": ["./node_modules/@angular/*"] }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
